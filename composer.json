        "ext-json": "*",
        "ext-openssl": "*",
        "ext-swoole": ">=4.4",
        "ext-iconv": "*",
        "graylog2/gelf-php": "~1.6.0"
    },
    "require-dev": {
    },
    "license": "MIT",
    "minimum-stability": "stable",
